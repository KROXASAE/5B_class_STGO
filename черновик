                        if (distance < 0) {
                            clearInterval(countDownFunction);
                            document.getElementById("timer").innerHTML = "–í—Ä–µ–º—è –∏—Å—Ç–µ–∫–ª–æ";
                         }
                        }, 1000)
                     </script> -->